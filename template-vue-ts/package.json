{
    "name": "meiyi-vue-typescript-starter",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview --port 5050",
        "typecheck": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "release": "rsync -avzr --delete --exclude node_modules --exclude '.git*' ./dist/ ubuntu@beta-2.meiyi:/home/ubuntu/help-center/"
    },
    "dependencies": {
        "vue": "^3.2.25",
        "@element-plus/icons-vue": "^1.1.4",
        "axios": "^0.26.0",
        "element-plus": "^2.0.5",
        "lodash-es": "^4.17.21",
        "pinia": "^2.0.11",
        "vue-axios": "^3.4.1",
        "vue-router": "^4.0.12"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.1.0",
        "@tailwindcss/line-clamp": "^0.3.1",
        "@tailwindcss/typography": "^0.5.2",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^16.11.25",
        "@vitejs/plugin-vue": "^2.2.2",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^10.0.0",
        "@vue/tsconfig": "^0.1.3",
        "autoprefixer": "^10.4.2",
        "eslint": "^8.5.0",
        "eslint-plugin-vue": "^8.2.0",
        "postcss": "^8.4.8",
        "prettier": "^2.5.1",
        "sass": "^1.49.9",
        "tailwindcss": "^3.0.23",
        "typescript": "~4.5.5",
        "unplugin-auto-import": "^0.6.1",
        "unplugin-vue-components": "^0.17.21",
        "vite": "^2.8.4",
        "vue-tsc": "^0.31.4"
    }
}